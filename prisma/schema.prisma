generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Resume {
  id         String @id @default(cuid())
  title      String
  name       String
  job        String
  address    String
  themeColor String
  summary    String
  experience Json
  skills     Json
  education  Json
  email      String @unique
}
